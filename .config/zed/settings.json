{
  "theme": {
    "dark": "Vercel Dark",
    "light": "Vercel Light"
  },
  "tabs": {
    "file_icons": true,
    "git_status": true
  },
  "buffer_font_features": {
    "liga": false,
    "calt": false
  },
  "terminal": {
    "line_height": "standard"
  },
  "auto_install_extensions": {
    "vercel-theme": true,
    "ruff": true,
    "biome": true,
    "emmet": true,
    "rainbow-csv": true,
    "codebook": true
  },
  "file_types": {
    "Shell Script": [".env.local", ".env.development"],
    "JSON": ["site.webmanifest"]
  },
  "lsp": {
    "biome": {
      "settings": {
        "require_config_file": true
      }
    }
  }
}
